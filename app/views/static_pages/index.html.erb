<h1>This is "<%= params[:user] %>" profile</h1>
<%= form_with(url:'/static_pages', method: 'get') do |f| %>
  <%= f.text_field :user, value: params[:user] %>
  <%= f.submit %>
<% end %>

<% if @images %>
  <% @images.each do |img| %>
  <br>
    <%= image_tag Flickr.url(img), loading: 'lazy' %>
   <%end%>
<% end %>



  <% flash.each do |type, msg| %>
  <br>
    <div style="color:red">
      <%= msg %>
    </div>
    <br>
  <% end %>
